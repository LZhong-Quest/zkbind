<window apply="test.ZKBindLoad1Composer">
	<custom-attributes composerName="vm"/>
<grid width="500px" >
	<columns>
		<column label="First Name"></column>
		<column label="Last Name"></column>
		<column label="Full Name"></column>
	</columns>
	<rows>
		<row id="row1">
			<label value="@bind(vm.p1.firstName)"/>
			<label value="@bind(vm.p1.lastName)"/>
			<label value="@bind(vm.p1.fullName)"/>
		</row>
	</rows>
</grid>
<button label="remove p1" onClick="@bind('removeP1')"/>
<button label="Dump" onClick="binder.getTracker().dump()"/>
</window>